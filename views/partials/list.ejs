<div class="col-sm-6 col-lg-4">
    <div class="listing-style1">
      <div class="list-thumb">
        <img class="w-100" src="<%= listprop.carousel[0].url %>" alt="">
        <%if(listprop.label){%>
          <div class="list-tag fz12"><span class="flaticon-electricity me-2"></span>PROMOTED</div>
          <%}%>
          <% if(!listprop.complete){%>
            <div class="list-tag fz12 bg-danger"><i class="fa-thin fa-triangle-exclamation pe-2"></i>Incomplete</div>
        <%}%>
        <div class="list-price">&#x20A6;<%= listprop.cost.toLocaleString() %> / <span>yr</span></div>
      </div>
      <div class="list-content">
        <h6 class="list-title"><a href="/details/<%= listprop._id %>"> <%= `${listprop.proptype == 'Apartment' ? listprop.rooms + " Bedroom Apartment" : listprop.proptype} `%></a></h6>
        <p class="list-text"><%= listprop.area + ", " + listprop.localgovs %></p>
        <div class="list-meta d-flex align-items-center">
          <a href="#"><span class="flaticon-bed"></span><%- listprop.rooms %> bed</a>
          <a href="#"><span class="flaticon-shower"></span><%= listprop.bathrooms %> bath</a>
          <!-- <a href="#"><span class="flaticon-expand"></span>1200 sqft</a> -->
        </div>
        <hr class="mt-2 mb-2">
        <!-- <div class="list-meta2 d-flex justify-content-between align-items-center">
          <span class="for-what">For Rent</span>
          <div class="icons d-flex align-items-center">
            <a href="/details/<%= listprop._id %>"><span class="flaticon-fullscreen"></span></a>
            <a href="#"><span class="flaticon-like"></span></a>
          </div>
        </div> -->
      </div>
    </div>
  </div>
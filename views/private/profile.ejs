 <%- include("./partials/header.ejs") -%>
 <!-- ### header end : -->

  <div class="dashboard_content_wrapper">
    <div class="dashboard dashboard_wrapper pr30 pr0-md">
       <!-- desktop side nav -->
       <%- include("./partials/desk&mobilesidenav.ejs", {desktopdisplay : true}) -%>
       <!-- desktop side nav:end -->
      <div class="dashboard__main pl0-md">
        <div class="dashboard__content property-page bgc-f7">
          <div class="row pb40 d-block d-lg-none">
            <div class="col-lg-12">
                <!-- mobile side nav -->
                <%- include("./partials/desk&mobilesidenav.ejs", {desktopdisplay : false}) -%>
                <!-- mobile side nav:end -->
            </div>
          </div>
          <div class="row align-items-center pb40">
            <div class="col-lg-12">
              <div class="dashboard_title_area">
                <h2> <%= !user.adminaccess ? 'My Profile' : 'Profile of ' + user.name.split(" ")[0] %></h2>
                <p class="text">We are glad to see you again!</p>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col-xl-12">
              <div class="ps-widget bgc-white bdrs12 default-box-shadow2 p30 mb30 overflow-hidden position-relative">
                <div class="col-lg-12 mb20">
                  <div class="ui-content">
                    <h4 class="mb20"> <%= user.name %></h4>
                    <p class="mb10 ff-heading">Email : <%= user.email %></p>
                    <p class="ff-heading"> Verified : <%= user.admin ? "Admin" : user.agent ? "Agent" : "User account" %></p>
                    <p class="ff-heading"> Your Listings: <%= ownprops.length %></p>
                    <p class="ff-heading"> Incomplete Listings: <%= ownprops.filter(p=> !p.complete).length %></p>
                  </div>
                  <% if(!user.admin && user.adminaccess){%>
                    <div class="col-sm-12 col-md-6">
                        <form class="text space fz15 mr10" method="POST" action="/admin/user/<%= user._id %>">
                          <button class="ud-btn btn-thm mr20" value="delete" name="type" type="submit">Delete</i></button>
                          <button class="ud-btn btn-fb" value="<%= user.disabled ? 'activate' : 'deactivate' %>" name="type" type="submit"> <%= user.disabled ? 'Activate' : 'Deactivate' %> </i></button>
                        </form>
                    </div>
                  <%}%>
          </div>
          </div>
        </div>

        <section class="pt0 pb90 bgc-f7">
          <div class="container">
            <div class="row">
              <% ownprops.forEach(listprop=>{ %>
                <%- include("../partials/list.ejs", {listprop}) -%>
              <%})%>
            </div>
